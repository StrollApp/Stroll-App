{
  "expo": {
    "name": "Stroll",
    "slug": "strollapp",
    "owner": "josephjin",
    "scheme": "com.animatorjoe.strollapp",
    "version": "0.1.10",
    "orientation": "portrait",
    "icon": "./assets/icon_itr_1.png",
    "splash": {
      "image": "./assets/splash_itr_1.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBFBFB"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.animatorjoe.strollapp",
      "buildNumber": "1.0.16",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location is required for Stroll to help you find the safest walks through Berkeley."
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA2xkTnpYdMaYkCcVNYSguzq0IGUeYKfZM"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_icon_itr_1.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./keys/google-services.json",
      "package": "com.animatorjoe.strollapp",
      "versionCode": 6,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "requireAuth": true
    }
  }
}
